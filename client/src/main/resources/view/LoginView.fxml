<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="210.0" prefWidth="559.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.phantom.netty.client.controller.LoginController">
    <children>
        <Label layoutX="45.0" layoutY="26.0" text="服务器地址:" />
        <ComboBox fx:id="serverUrl" layoutX="134.0" layoutY="22.0" prefHeight="23.0" prefWidth="382.0" editable="true" />

        <Label layoutX="45.0" layoutY="62.0" text="端口:" />
        <TextField fx:id="port" layoutX="134.0" layoutY="58.0" prefHeight="23.0" prefWidth="382.0" />

        <Label layoutX="45.0" layoutY="98.0" text="用户名:" />
        <TextField fx:id="username" layoutX="134.0" layoutY="94.0" prefHeight="23.0" prefWidth="382.0" />

        <Label layoutX="45.0" layoutY="134.0" text="密码:" />
        <PasswordField fx:id="password" layoutX="134.0" layoutY="130.0" prefHeight="23.0" prefWidth="382.0" />

        <CheckBox fx:id="remember" layoutX="134.0" layoutY="171.0" mnemonicParsing="false" text="记住信息" />
        <Button fx:id="connect" layoutX="354.0" layoutY="167.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="74.0" text="连接" />
        <Button fx:id="exit" layoutX="442.0" layoutY="167.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="74.0" text="取消" />
    </children>
</AnchorPane>
